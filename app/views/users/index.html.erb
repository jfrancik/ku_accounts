<h1>Listing users</h1>

<table>
  <tr>
    <th>K number</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Password</th>
    <th>Privilege</th>
    <th>Key</th>
    <th>Ver</th>
    <th>Salt</th>
    <th>Passwd reset</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.k_number %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.password %></td>
    <td><%= user.privilege %></td>
    <td><%= user.key %></td>
    <td><%= user.ver %></td>
    <td><%= user.salt %></td>
    <td><%= user.passwd_reset %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
